{
  "knex": {
    "client": "sqlite3",
    "connection": {
      "filename": ":memory:"
    },
    "pool": {
      "min": 1,
      "max": 1,
      "disposeTimeout": 120000,
      "idleTimeoutMillis": 120000
    },
    "debug": false,
    "useNullAsDefault": true
  },

  "winston": {
    "console": {
      "level": "info"
    }
  }
}
